  "bugs": {
    "url": "https://github.com/qwertie/simplertime/issues"
  },
  "homepage": "https://github.com/qwertie/simplertime#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.3.6",
    "package-preview": "^1.0.6",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.2"
  },
  "files": [
    "readme.md",
    "simplertime.ts",
    "dist/simplertime.*"
  ]
